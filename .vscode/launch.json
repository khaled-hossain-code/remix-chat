{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Remix: Node",
          "type": "pwa-node",
          "request": "launch",
          "runtimeExecutable": "npm",
          "runtimeArgs": ["run", "dev"],
          "skipFiles": ["<node_internals>/**"]
      },
      {
          "name": "Remix: Chrome",
          "type": "pwa-chrome",
          "request": "launch",
          "url": "http://localhost:3000",
          "webRoot": "${workspaceFolder}"
      }
  ]
}
